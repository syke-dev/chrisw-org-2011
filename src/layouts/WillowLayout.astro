---
import BaseLayout from '@layouts/BaseLayout.astro';
import { default as Image, Fit } from '@components/Image.astro';
---

<BaseLayout title='Watt Bodies'>

	<Fragment slot="header_image">
		<Image src='/willow/logo.gif' width={400} height={100} />
	</Fragment>

	<Fragment slot="base_body">

		<slot name="body" />

		<div id = "project_navigation">
		
			<a href = "/willow">Home</a>
			<a href = "/willow/info">Info</a>
			<a href = "/willow/media">Media</a>
			<a href = "/willow/download">Download</a>
		
		</div>

	</Fragment>

</BaseLayout>